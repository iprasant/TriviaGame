    speechOutput += repromptText;\
    sessionAttributes = \{\
        "speechOutput": repromptText,\
        "repromptText": repromptText,\
        "currentQuestionIndex": currentQuestionIndex,\
        "correctAnswerIndex": correctAnswerIndex + 1,\
