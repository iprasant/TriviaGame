                correctAnswerIndex = Math.floor(Math.random() * (ANSWER_COUNT));\
                var roundAnswers = populateRoundAnswers(gameQuestions, currentQuestionIndex, correctAnswerIndex),\
\
                    questionIndexForSpeech = currentQuestionIndex + 1,\
